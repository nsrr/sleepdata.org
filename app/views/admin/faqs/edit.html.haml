- @title = "Edit FAQ - #{@faq.question_was}"

- content_for :header, @faq.question_was
= render "internal/tabs"
- content_for :breadcrumbs do
  %ol
    %li= link_to "faqs", admin_faqs_path
    %li= link_to @faq.question_was.downcase, admin_faq_path(@faq)
    %li.breadcrumb-muted edit
- content_for :sidebar, render("admin/sidebar")

.dashboard-container= render "admin/faqs/form", faq: @faq
